       mutate (ncat1=apply(data1_cat1, 1, function(x) length(which(x==1))),
               ncat2=apply(data1_cat1, 1, function(x) length(which(x==2))),
               ncat3=apply(data1_cat1, 1, function(x) length(which(x==3))),
              
               ncat1pct=ifelse(is.na(ncat1), 0, ncat1/5),
               ncat2pct=ifelse(is.na(ncat2), 0, ncat2/5),
               ncat3pct=ifelse(is.na(ncat3), 0, ncat3/5),
               
               logpct1=ifelse(ncat1pct==0, 0, log(ncat1pct)),
               logpct2=ifelse(ncat2pct==0, 0, log(ncat1pct)),
               logpct3=ifelse(ncat3pct==0, 0, log(ncat1pct)),
               
                H=(ncat1pct*logpct1+ ncat2pct*logpct2+ncat3pct*logpct3))
      

