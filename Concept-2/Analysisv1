#New variable for hrt use
#1 current, 2 former, 3 is ever (unsure)
data1$hrt_use<-NA
data1$hrt_use[which (data1$hrtuse==0)]<-1
data1$hrt_use[which (data1$hrtuse==1)]<-2
data1$hrt_use[which (data1$hrtuse==2)]<-3
