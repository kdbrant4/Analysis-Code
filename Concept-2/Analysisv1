    ######################
    # Create rr categories
    ######################
    
      #<15%, 15-20%, >20% RR
      #Lifetime risk of US (White), ~13%
      #RR of 1.15, 1.15-1.54, >1.54
    
      data1$gail_rr_cat<-as.numeric(cut(data1$gail_rr, breaks=c(0, 1.15, 1.54, 10)))
    
      data1$rosner_logrel_center<-as.numeric(cut(data1$rosner_rr, breaks=c(0, 1.15, 1.54, 10)))
    
      data1$icare_rr_cat<-as.numeric(cut(data1$icare_rr, breaks=c(0, 1.15, 1.54, 10)))
         
      data1$tyrer_rr_cat<-as.numeric(cut(data1$tyrer_rr, breaks=c(0, 1.15, 1.54, 10)))
      
      data1$boad_rr_cat<-as.numeric(cut(data1$boad_rr, breaks=c(0, 1.15, 1.54, 10)))
      
      ##Black participants
