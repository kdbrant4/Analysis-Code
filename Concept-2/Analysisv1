    table(data1$cohort)
    data1nhs<-data1[which (data1$cohort=="NHS"),]
    data1nhs2<-data1[which (data1$cohort=="NHS2"),]
    data1plco<-data1[which (data1$cohort=="PLCO"),]

#Within cohorts- set log rel risk and rr based on means only in cohorts
    
    
    data1nhs$gail_logrel<-data1nhs$gail_agemenarch+data1nhs$gail_biopage+data1nhs$gail_agerel+data1nhs$gail_atyphyper+data1nhs$gail_biopsy
    #Center the logrel risk for comparisons
    data1nhs$gail_logrel_center<-data1nhs$gail_logrel-mean(data1nhs$gail_logrel)
    data1nhs$gail_rr<-exp(data1nhs$gail_logrel_center)
    
    data1nhs$care_logrel<-data1nhs$care_agemenarch+data1nhs$care_biopage+data1nhs$care_agerel+data1nhs$care_atyphyper+data1nhs$care_biopsy
    data1nhs$BWHS_logrel<-data1nhs$BWHS_agemenarch50+data1nhs$BWHS_famhx_age50+data1nhs$BWHS_ocuse+data1nhs$BWHS_bmi_young+data1nhs$BWHS_bmi+data1nhs$BWHS_bbdbiopsy
    data1nhs$asian_logrel<-data1nhs$asian_agemenarch+data1nhs$asian_biopage+data1nhs$asian_agerel+data1nhs$asian_atyphyper+data1nhs$asian_biopsy
    
    data1nhs$rosner_logrel<-data1nhs$rosner_agemeno+data1nhs$rosner_postmeno_dur_new+data1nhs$rosner_bbd+data1nhs$rosner_famhx+data1nhs$rosner_pmh+data1nhs$rosner_bmi+data1nhs$rosner_height+data1nhs$rosner_alcohol
    data1nhs$rosner_logrel_center<-data1nhs$rosner_logrel-mean(data1nhs$rosner_logrel)
    data1nhs$rosner_rr<-exp(data1nhs$rosner_logrel_center)
    
    data1nhs$icare_logrel<-NA
    data1nhs$icare_logrel<-data1nhs$icare_agemenarch+data1nhs$icare_agemeno+data1nhs$icare_parity+ data1nhs$icare_height*(data1nhs$heightnew-162.3) +data1nhs$icare_agefb+data1nhs$icare_ocuse+data1nhs$icare_bmi+data1nhs$icare_alcohol+data1nhs$icare_bbd+data1nhs$icare_bmihrt
    data1nhs$icare_logrel_center<-data1nhs$icare_logrel-mean(data1nhs$icare_logrel)
    data1nhs$icare_rr<-exp(data1nhs$icare_logrel_center)
    
    data1nhs$tyrer_logrel<-(data1nhs$tyrer_agemenarch*(data1nhs$agemenarche_new2-13))+(data1nhs$tyrer_agemeno*(data1nhs$meno_age_new2-60))+ data1nhs$tyrer_height+ data1nhs$tyrer_agefb + data1nhs$tyrer_bmimeno + data1nhs$tyrer_hrtuse +
      data1nhs$tyrer_biopage+data1nhs$tyrer_famhx
    summary(data1nhs$tyrer_logrel)
    
    #Center the logrel risk for comparisons
    data1nhs$tyrer_logrel_center<-data1nhs$tyrer_logrel-mean(data1nhs$tyrer_logrel)
    data1nhs$tyrer_rr<-exp(data1nhs$tyrer_logrel_center)
    
    data1nhs$boad_logrel<-data1nhs$boad_agemenarche+data1nhs$boad_agemeno+ data1nhs$boad_parity+ data1nhs$boad_ageflb + data1nhs$boad_ocuse + data1nhs$boad_pmh +data1nhs$boad_bmi+data1nhs$boad_height+data1nhs$boad_alc+data1nhs$boad_famhx
    #Center the logrel risk for comparisons
    data1nhs$boad_logrel_center<-data1nhs$boad_logrel-mean(data1nhs$boad_logrel)
    data1nhs$boad_rr<-exp(data1nhs$boad_logrel_center)
    
    
    ###NHS2
    
    
