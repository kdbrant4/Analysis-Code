#2. Number of biopsies
data1$biopsy_num<-NA
data1$biopsy_num[which (data1$Biopsies_number!=888)]<-data1$Biopsies_number
#No counts of biopsy #
#change to 1 for those with yes no=1
data1$biopsy_num[which (is.na(data1$biopsy_num) & data1$Biopsies_yesno==1)]<-1
