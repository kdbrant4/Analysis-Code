      rankcor3<-cor(data1_cont1$gail_logrel_center, data1_cont1$tyrer_logrel_center, method="spearman")
      rankcor4<-cor(data1_cont1$gail_logrel_center, data1_cont1$boad_logrel_center, method="spearman")
      rankcor5<-cor(data1_cont1$rosner_logrel_center, data1_cont1$icare_logrel_center, method="spearman")
      rankcor6<-cor(data1_cont1$rosner_logrel_center, data1_cont1$tyrer_logrel_center, method="spearman")
      rankcor7<-cor(data1_cont1$rosner_logrel_center, data1_cont1$boad_logrel_center, method="spearman")
      rankcor8<-cor(data1_cont1$icare_logrel_center, data1_cont1$tyrer_logrel_center, method="spearman")
      rankcor9<-cor(data1_cont1$icare_logrel_center, data1_cont1$boad_logrel_center, method="spearman")
      rankcor10<-cor(data1_cont1$tyrer_logrel_center, data1_cont1$boad_logrel_center, method="spearman")
      
      #As table
      rankcor<-as.data.frame(rbind(rankcor1, rankcor2, rankcor3, rankcor4, rankcor5, rankcor6, rankcor7, rankcor8, rankcor9, rankcor10))
      names(rankcor)<-c("spearman cor")
      rownames(rankcor)<-c("gail.v.rosner", "gail.v.icare","gail.v.tyrer","gail.v.boad","rosner.v.icare","rosner.v.tyrer","rosner.v.boad","icare.v.tyrer","icare.v.boad","tyrer.v.boad")
      write.csv(rankcor,"C:/Users/krist/Dropbox/BCRP Project/rankcorrtable.csv")
