      #Cohen's kappa
      
      compare1<-cbind(data1$gail_rr_cat, data1$rosner_rr_cat)
      compare2<-cbind(data1$gail_rr_cat, data1$icare_rr_cat)
      compare3<-cbind(data1$gail_rr_cat, data1$tyrer_rr_cat)
      compare4<-cbind(data1$gail_rr_cat, data1$boad_rr_cat)
      compare5<-cbind(data1$rosner_rr_cat, data1$icare_rr_cat)
      compare6<-cbind(data1$rosner_rr_cat, data1$tyrer_rr_cat)
      compare7<-cbind(data1$rosner_rr_cat, data1$boad_rr_cat)
      compare8<-cbind(data1$icare_rr_cat, data1$tyrer_rr_cat)
      compare9<-cbind(data1$icare_rr_cat, data1$boad_rr_cat)
      compare10<-cbind(data1$tyrer_rr_cat, data1$boad_rr_cat)
      
      
      install.packages("psych")
      library("psych")
      
      kappa1<-cohen.kappa(compare1)
      kappa2<-cohen.kappa(compare2)
      kappa3<-cohen.kappa(compare3)
      kappa4<-cohen.kappa(compare4)
      kappa5<-cohen.kappa(compare5)
      kappa6<-cohen.kappa(compare6)
      kappa7<-cohen.kappa(compare7)
      kappa8<-cohen.kappa(compare8)
      kappa9<-cohen.kappa(compare9)
      kappa10<-cohen.kappa(compare10)
      
      kappapull= function(x){
        result<-matrix(NA,nrow=1, ncol=4)
        result[1,1]<-x$weighted.kappa
        result[1,2]<-var<-x$var.weighted
        result[1,3]<- lci<-x$confid[2,1]
        result[1,4]<- rci<-x$confid[2,3]
        return(result)
      }
       dat1<-kappapull(kappa1)
       dat2<-kappapull(kappa2)
       dat3<-kappapull(kappa3)
       dat4<-kappapull(kappa4)
       dat5<-kappapull(kappa5)
       dat6<-kappapull(kappa6)
       dat7<-kappapull(kappa7)
       dat8<-kappapull(kappa8)
       dat9<-kappapull(kappa9)
       dat10<-kappapull(kappa10)
       
       #print table
       
       kappatable<-as.data.frame(rbind(dat1, dat2, dat3, dat4, dat5, dat6, dat7, dat8, dat9, dat10))
       names(kappatable)<-c("kappa","var","lci","rci")
       rownames(kappatable)<-c("gail.v.rosner", "gail.v.icare","gail.v.tyrer","gail.v.boad","rosner.v.icare","rosner.v.tyrer","rosner.v.boad","icare.v.tyrer","icare.v.boad","tyrer.v.boad")
       kappatable
       write.csv(kappatable,"C:/Users/krist/Dropbox/BCRP Project/kappatable.csv")
       kappatable
       
    
      #Rank correlation
       
      rankcor1<-cor(data1_cont1$gail_logrel_center, data1_cont1$rosner_logrel_center, method="spearman")
