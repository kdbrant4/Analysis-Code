  ggsave("C:/Users/krist/Dropbox/BCRP Project/stage_atdx.png", width=4, height=4)
  
  ggplot(data1_case_cat, aes(x=as.factor(er_primary1)))+ geom_bar(color="lightsteelblue2", fill="lightsteelblue2") + ylab("Frequency")+xlab("ER status")+ theme(plot.title=element_text(size=12),axis.title.x=element_text(size=14),axis.title.y=element_text(size=14),axis.text.y=element_text(size=14), axis.text.x=element_text(size=14))
  ggsave("C:/Users/krist/Dropbox/BCRP Project/stage_atdx.png", width=4, height=4)
  
