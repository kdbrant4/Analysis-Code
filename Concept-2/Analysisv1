      bcompare15<-cbind(data1_black_cat1$icare_rr_cat, data1_black_cat1$BWHS_rr_cat)
      bcompare16<-cbind(data1_black_cat1$tyrer_rr_cat, data1_black_cat1$boad_rr_cat)
      bcompare17<-cbind(data1_black_cat1$tyrer_rr_cat, data1_black_cat1$care_rr_cat)
      bcompare18<-cbind(data1_black_cat1$tyrer_rr_cat, data1_black_cat1$BWHS_rr_cat)
      bcompare19<-cbind(data1_black_cat1$boad_rr_cat, data1_black_cat1$care_rr_cat)
      bcompare20<-cbind(data1_black_cat1$boad_rr_cat, data1_black_cat1$BWHS_rr_cat)
      bcompare21<-cbind(data1_black_cat1$care_rr_cat, data1_black_cat1$BWHS_rr_cat)

      bkappa1<-cohen.kappa(bcompare1)
      bkappa2<-cohen.kappa(bcompare2)
      bkappa3<-cohen.kappa(bcompare3)
      bkappa4<-cohen.kappa(bcompare4)
      bkappa5<-cohen.kappa(bcompare5)
      bkappa6<-cohen.kappa(bcompare6)
      bkappa7<-cohen.kappa(bcompare7)
      bkappa8<-cohen.kappa(bcompare8)
      bkappa9<-cohen.kappa(bcompare9)
      bkappa10<-cohen.kappa(bcompare10)
      bkappa11<-cohen.kappa(bcompare11)
      bkappa12<-cohen.kappa(bcompare12)
      bkappa13<-cohen.kappa(bcompare13)
      bkappa14<-cohen.kappa(bcompare14)
      bkappa15<-cohen.kappa(bcompare15)
      bkappa16<-cohen.kappa(bcompare16)
      bkappa17<-cohen.kappa(bcompare17)
      bkappa18<-cohen.kappa(bcompare18)
      bkappa19<-cohen.kappa(bcompare19)
      bkappa20<-cohen.kappa(bcompare20)
      bkappa21<-cohen.kappa(bcompare21)
      
      kappapull= function(x){
        result<-matrix(NA,nrow=1, ncol=4)
        result[1,1]<-x$weighted.kappa
        result[1,2]<-var<-x$var.weighted
        result[1,3]<- lci<-x$confid[2,1]
        result[1,4]<- rci<-x$confid[2,3]
        return(result)
      }
      dat1<-kappapull(bkappa1)
      dat2<-kappapull(bkappa2)
      dat3<-kappapull(bkappa3)
      dat4<-kappapull(bkappa4)
      dat5<-kappapull(bkappa5)
      dat6<-kappapull(bkappa6)
      dat7<-kappapull(bkappa7)
      dat8<-kappapull(bkappa8)
      dat9<-kappapull(bkappa9)
      dat10<-kappapull(bkappa10)
      dat11<-kappapull(bkappa11)
      dat12<-kappapull(bkappa12)
      dat13<-kappapull(bkappa13)
      dat14<-kappapull(bkappa14)
      dat15<-kappapull(bkappa15)
      dat16<-kappapull(bkappa16)
      dat17<-kappapull(bkappa17)
      dat18<-kappapull(bkappa18)
      dat19<-kappapull(bkappa19)
      dat20<-kappapull(bkappa20)
      dat21<-kappapull(bkappa21)
      
      #print table
      
      bkappatable<-as.data.frame(rbind(dat1, dat2, dat3, dat4, dat5, dat6, dat7, dat8, dat9, dat10,dat11, dat12, dat13, dat14, dat15, dat16, dat17,dat18, dat19,dat20,dat21))
      names(bkappatable)<-c("kappa","var","lci","rci")
      rownames(bkappatable)<-c("gail.v.rosner", "gail.v.icare","gail.v.tyrer","gail.v.boad","gail.v.care","gail.v.bwhs","rosner.v.icare","rosner.v.tyrer","rosner.v.boad","rosner.v.care","rosner.v.bwhs","icare.v.tyrer","icare.v.boad","icare.v.care","icare.v.bwhs",
                           "tyrer.v.boad","tyrer.v.care","tyrer.v.bwhs","boad.v.care","boad.v.bwhs","care.v.bwhs")
      write.csv(bkappatable,"C:/Users/krist/Dropbox/BCRP Project/kappatable_black.csv")
     dim(data1_black_cat1)
      
      bkappatable
      #Rank correlation
      brankcor1<-cor(data1_black_cont1$gail_logrel_center, data1_black_cont1$rosner_logrel_center, method="spearman")
