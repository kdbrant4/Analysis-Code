tblFun <- function(x){
  tbl <- table(x, useNA="always")
  res <- cbind(tbl,round(prop.table(tbl)*100,2))
  colnames(res) <- c('Count','Percentage')
  res
}

#summaries overall
summariescat<-lapply(data1_categories[1:35],tblFun)
summariescat
dim(data1_categories)
