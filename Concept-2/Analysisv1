      brankcor7<-cor(data1_black_cont1$rosner_logrel_center, data1_black_cont1$icare_logrel_center, method="spearman")
      brankcor8<-cor(data1_black_cont1$rosner_logrel_center, data1_black_cont1$tyrer_logrel_center, method="spearman")
      brankcor9<-cor(data1_black_cont1$rosner_logrel_center, data1_black_cont1$boad_logrel_center, method="spearman")
