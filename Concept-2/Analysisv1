      ############################
      #3. Distance from agreement
      ############################
      
      head(data1_cat1new)
      #Let v_j=(v_j1,...,v_j5), where v_jk is the log RR for subject j for model k. 
      #Then the distance is sqrt(t(a) %*% a), where a = v - s*(t(v) %*% s)/(t(s) %*% s) and s = c(1,1,1,1,1). 
      #a is v-average(v) 
      #Does not require categorization. Measures how far the actual log RRs are from perfect agreement (where all 5 models give the same results) on a continuous scale.
      

      data1_cont1$sum<-data1_cont1$gail_logrel_center+data1_cont1$rosner_logrel_center+data1_cont1$icare_logrel_center+data1_cont1$tyrer_logrel_center+data1_cont1$boad_logrel_center
      data1_cont1$mean<-data1_cont1$sum/5
      
      data1_cont1$diff_gail<-data1_cont1$mean-data1_cont1$gail_logrel_center
      data1_cont1$diff_rosner<-data1_cont1$mean-data1_cont1$rosner_logrel_center
      data1_cont1$diff_icare<-data1_cont1$mean-data1_cont1$icare_logrel_center
      data1_cont1$diff_tyrer<-data1_cont1$mean-data1_cont1$tyrer_logrel_center
      data1_cont1$diff_boad<-data1_cont1$mean-data1_cont1$boad_logrel_center
      
      
      data1_cont1$var=(data1_cont1$diff_gail)^2+(data1_cont1$diff_rosner)^2+(data1_cont1$diff_icare)^2+ (data1_cont1$diff_tyrer)^2+(data1_cont1$diff_boad)^2
      
