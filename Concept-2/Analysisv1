      data1_black$BWHS_rr_cat<-as.numeric(cut(data1_black$BWHS_rr, breaks=c(0, 1.15, 1.54, 10)))
                 
      data1_black$care_rr_cat<-as.numeric(cut(data1_black$care_rr, breaks=c(0, 1.15, 1.54, 10)))
                            
      
      #Cohort specific
      
