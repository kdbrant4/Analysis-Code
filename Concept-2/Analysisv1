      brankcor12<-cor(data1_black_cont1$icare_logrel_center, data1_black_cont1$tyrer_logrel_center, method="spearman")
      brankcor13<-cor(data1_black_cont1$icare_logrel_center, data1_black_cont1$boad_logrel_center, method="spearman")
      brankcor14<-cor(data1_black_cont1$icare_logrel_center, data1_black_cont1$care_logrel_center, method="spearman")
      brankcor15<-cor(data1_black_cont1$icare_logrel_center, data1_black_cont1$BWHS_logrel_center, method="spearman")
      brankcor16<-cor(data1_black_cont1$tyrer_logrel_center, data1_black_cont1$boad_logrel_center, method="spearman")
